{
  "experiment_name": "q002_baseline",
  "description": "Official IRED baseline on matrix inversion (standard configuration)",
  "seed": 42,
  "task": "inverse",
  "model": {
    "type": "energy_based",
    "input_dim": 400,
    "output_dim": 400,
    "hidden_dim": 512
  },
  "diffusion": {
    "steps": 10,
    "beta_schedule": "cosine",
    "clip_denoised": true
  },
  "training": {
    "batch_size": 2048,
    "learning_rate": 1e-4,
    "train_steps": 100000,
    "ema_decay": 0.995,
    "weight_decay": 0.0
  },
  "dataset": {
    "rank": 20,
    "num_samples": 100000
  },
  "validation": {
    "validate_interval": 1000,
    "num_validation_samples": 5000
  },
  "output": {
    "save_artifacts": true,
    "log_interval": 500,
    "checkpoint_interval": 5000
  }
}
